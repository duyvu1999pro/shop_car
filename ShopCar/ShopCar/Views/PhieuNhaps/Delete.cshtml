@model ShopCar.Model.PhieuNhap

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Xóa</h2>

<h3>Bạn có muốn xóa không ?</h3>
<div>
    <h4>Phiếu nhập</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Mã phiếu nhập
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MaPN)
        </dd>
        <dt>
            Ngày nhập
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NgayNhap)
        </dd>

        <dt>
            Nhà cung cấp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NhaCC.TenNCC)
        </dd>
        <dt>
            Mã nhà cung cấp
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NhaCC.MaNCC)
        </dd>


    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa" class="btn btn-danger" /> |
            @Html.ActionLink("Quay lại danh sách", "Index")
        </div>
    }
</div>
